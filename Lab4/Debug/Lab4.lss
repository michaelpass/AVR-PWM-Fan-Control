00008d f409                      	brne use_regular
00008e 2f89                      	mov r24, r25
                                 use_regular:
00008f 9586                      	lsr r24	; Divide by 2. OCR0B values range from 1 to 199. This now means they range from 0 to 99. PWM percentage can be directly taken from this number.
000090 2f08                      	mov dd8u, r24
000091 e01a                      	ldi dv8u, 10 ; Divide by 10 to get digits
                                 ; Store null-terminated string
000092 e080                      	ldi r24, 0x00
000093 9380 0104                 	sts dtxt+4, r24
                                 ; Store 0
000095 e380                      	ldi r24, 0x30 ; 0 in ASCII
000096 9380 0103                 	sts dtxt+3, r24
                                 ; Store decimal point
000098 e28e                      	ldi r24, 0x2E
000099 9380 0102                 	sts dtxt+2, r24
                                 ; Divide percentage by 10 and format remainder
00009b d089                      	rcall div8u ; Remainder is stored in drem8u (r15), Result is stored in dres8u (r16)
00009c e380                      	ldi r24, 0x30 ; ASCII offset
00009d 0ef8                      	add drem8u, r24 ; Covert to ASCII
00009e 92f0 0101                 	sts dtxt+1, drem8u
                                 ; Store remaining digit.
0000a0 0f08                      	add dres8u, r24 ; Covert to ASCII
0000a1 9300 0100                 	sts dtxt, dres8u
0000a3 9508                      	ret
                                 
